// Declaration
function* foo(x) {
    var y = 2 * (yield (x + 1));
    var z = yield (y / 3);
    return (x + y + z);
}

// Do nothing
var it = foo( 5 );

console.log( it.next() );      // { value:6, done:false }
console.log( it.next( 12 ) );  // { value:8, done:false }
console.log( it.next( 1 ) );   // { value:30, done:true } 
                               // (x=5 + y=(2*12) + z=1) = 30
